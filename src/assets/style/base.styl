*
  margin 0 auto
  padding 0
  list-style none
  box-sizing: border-box

body, html
  line-height 1
  font-family centrale_sans_book, tahoma, arial, helvetica, sans-serif
  background-color #F2F2F2
  overflow hidden
  height 100%

.icon
  width 1em
  height 1em
  vertical-align middle
  fill currentColor
  overflow hidden

.btc
  overflow hidden

.relative
  position relative

.clearfix
  &:after
    display: block
    content: "."
    height: 0
    line-height: 0
    clear: both
    visibility: hidden

.ellipsis{white-space:nowrap; text-overflow:ellipsis; overflow: hidden}

.box-shadow
  box-shadow 3px 3px 10px 0px rgba(0, 0, 0, 0.2)

a
  text-decoration none

input:-webkit-autofill,
textarea:-webkit-autofill,
select:-webkit-autofill
  -webkit-box-shadow 0 0 0px 1000px #ffffff inset !important
  -ms-box-shadow 0 0 0px 1000px #ffffff inset !important

#fileTable_wrapper table.dataTable tbody th, 
#fileTable_wrapper table.dataTable tbody td
	border-bottom 1px solid #ebebeb !important
	border-right 1px solid #ebebeb !important

#fileTable_wrapper table.dataTable tbody th, 
#fileTable_wrapper table.dataTable tbody td
  padding 10px 0px
  color #717071
#fileTable_wrapper .dataTables_wrapper
  position static
#upLoadBox
  position relative
#fileTable_wrapper #fileTable_paginate
  margin 40px 0 0
  position absolute;
  bottom -70px
  left 50%
  transform: translateX(-50%)
#fileTable_wrapper .dataTables_wrapper .dataTables_paginate .paginate_button.current, 
#fileTable_wrapper .dataTables_wrapper .dataTables_paginate .paginate_button.current:hover
	  color #777!important
    border medium
    background #fff!important
#fileTable_wrapper .dataTables_wrapper .dataTables_paginate .paginate_button.current, 
#fileTable_wrapper .dataTables_wrapper .dataTables_paginate .paginate_button.current:hover
    color #777!important
    border medium
    background #fff!important

